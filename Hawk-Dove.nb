Cell[BoxData[
 RowBox[{
  RowBox[{"nextGen", "[", 
   RowBox[{
   "zpop_", ",", "\[Mu]_", ",", "\[Lambda]_", ",", "gain_", ",", "cost_", ",",
     "n_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"mutpop", ",", "zcorr", ",", "gamepayoffs", ",", "repro"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"mutpop", "=", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"mut", "[", 
          RowBox[{"#", ",", "\[Mu]", ",", "\[Lambda]"}], "]"}], "&"}], ",", 
        "zpop"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"zcorr", "=", 
      RowBox[{
       RowBox[{"Total", "[", "zpop", "]"}], "/", 
       RowBox[{"(", 
        RowBox[{"n", "-", "1"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"gamepayoffs", "=", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"payoff", "[", 
           RowBox[{"#", ",", "zcorr", ",", "n"}], "]"}], "&"}], ",", "zpop"}],
         "]"}], "//.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"v", "\[Rule]", "gain"}], ",", 
         RowBox[{"c", "\[Rule]", "cost"}]}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"repro", "=", 
      RowBox[{"RandomChoice", "[", 
       RowBox[{
        RowBox[{"gamepayoffs", "\[Rule]", "mutpop"}], ",", "n"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "repro", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
